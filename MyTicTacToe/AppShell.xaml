<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MyTicTacToe.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MyTicTacToe.MVVM.View"
    Shell.FlyoutBehavior="Disabled">


    <ShellContent Route="Start" Title="Start" ContentTemplate="{DataTemplate local:StartView}"/>
    <ShellContent Route="TicTacToe" Title="TicTacToe" ContentTemplate="{DataTemplate local:TicTacView}"/>
    <ShellContent Route="HeighScore" Title="HeighScore" ContentTemplate="{DataTemplate local:HeighscoreView}"/>

</Shell>
